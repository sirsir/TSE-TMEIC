INSERT INTO d_shipping_ticket_print_queue(
            create_date, update_date, printer_id, sequence_no, print_type, 
            customer_name, part_name, part_no, lot_no, mfg_date, plan_code, 
            time_of_delivery, checker, packing_date, model, color, serial_no, 
            quantity, location, quality_rank)
    VALUES (CURRENT_TIMESTAMP,
            CURRENT_TIMESTAMP,
            /*printerId*/,
            (select extract(epoch from date_trunc('milliseconds', CURRENT_TIMESTAMP)) * 1000),
            /*printType*/,
            /*customerName*/,
            /*partName*/,
            /*partNo*/,
            /*lotNo*/,
            /*mfgDate*/,
            /*planCode*/,
            /*timeOfDelivery*/,
            /*checker*/,
            /*packingDate*/,
            /*model*/,
            /*color*/,
            /*serialNo*/,
            /*quantity*/,
            /*location*/,
            /*qualityRank*/)